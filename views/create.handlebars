<div id="createPage">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h2>Unwrapped - This is our create Page!</h2>

        <form>
            <div class="form-group">
              <label>Name</label>
              <input type="name" class="form-control" id="createName" aria-describedby="emailHelp">
            </div>

            <div class="form-group">
              <label>Occasion</label>
              <select id="inputState" class="form-control">
                <option selected>Choose...</option>
                <option>Bridal</option>
                <option>Wedding</option>
                <option>Baby</option>
                <option>Birthday</option>
              </select>
            </div>

            <div class="form-group">
              <label>Date</label>
              <small></small>
              <input type="date" class="form-control" id="createDate" placeholder="MM/DD/YYYY">
            </div>

            <button type="submit" class="btn btn-primary">Submit</button>
          </form>


      </div>
    </div>
  </div>
</div>

